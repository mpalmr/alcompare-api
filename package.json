{
  "name": "@alcompare/api",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "license": "UNLICENSED",
  "author": "Matthew Palmer <mspalmer91@gmail.com>",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec \"node -r ts-node/register --inspect\" ./src/index.ts",
    "test": "yarn typecheck && yarn lint && yarn jest",
    "typecheck": "tsc",
    "lint": "eslint --ext ts,js .",
    "jest": "NODE_ENV=test jest -i",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "knex migrate:make -x ts",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make -x ts",
    "db:reset": "knex migrate:rollback --all && yarn db:migrate && yarn db:seed"
  },
  "dependencies": {
    "argon2": "^0.40.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-promise-router": "^4.1.1",
    "knex": "^3.1.0",
    "objection": "^3.1.4",
    "pg": "^8.12.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^28.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
